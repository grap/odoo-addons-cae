<odoo>

    <record id="product_template_mother" model="product.template">
        <field name="name">CAE - Mother Product Template</field>
        <field name="company_id" ref="fiscal_company_base.company_fiscal_mother"/>
        <field name="categ_id" ref="product.product_category_1"/>
    </record>

    <record id="product_template_child" model="product.template">
        <field name="name">CH1 - Child 1 Product Template</field>
        <field name="company_id" ref="fiscal_company_base.company_fiscal_child_1"/>
        <field name="categ_id" ref="product.product_category_1"/>
    </record>

    <record id="product_template_administrative" model="product.template">
        <field name="name">CAE - Administrative Mother Product Template</field>
        <field name="company_id" ref="fiscal_company_base.company_fiscal_mother"/>
        <field name="categ_id" ref="product.product_category_all"/>
        <field name="cae_administrative_ok" eval="True"/>
    </record>

</odoo>
